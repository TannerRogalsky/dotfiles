[user]
  name = Tanner Rogalsky
  email = tanner@tannerrogalsky.com
[alias]
  c = commit
  co = checkout
  sub = submodule update --init --recursive
  statys = status
  l = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = green
  local = yellow
  remote = green
[color "diff"]
  meta = yellow
  frag = magenta
  old = red
  new = green
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[push]
  default = current
